{
    "name": "timkippdev/slack-notifier",
    "description": "PHP client built for integrating with Slack's Incoming Webhooks",
    "type": "library",
    "license": "MIT",
    "keywords": ["timkippdev", "slack", "notifier", "incoming", "webhooks"],
    "authors": [
      {
        "name": "Tim Kipp",
        "email": "timkipp.22.developer@gmail.com"
      }
    ],
    "require": {
      "guzzlehttp/guzzle": "~6.0",
      "php": "^7.1.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^7"
    },
    "autoload" : {
      "psr-4" : {
        "TimKippDev\\SlackNotifier\\" : "src/"
      }
    },
    "autoload-dev" : {
      "psr-4" : {
        "Tests\\" : "tests/"
      }
    },
    "config": {
      "discard-changes": true,
      "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
  }
